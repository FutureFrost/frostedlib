{
	"schemaVersion": 1,
	"id": "frostedlib",
	"version": "${version}",
	"name": "frostedlib",
	"description": "A library for Origins teleportation and position management.",
	"authors": [
		"Future Frost"
	],
	"contact": {
		"sources": "https://github.com/FutureFrost/frostedlib"
	},
	"license": "MIT",
	"icon": "assets/frostedlib/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.futurefrost.frostedlib.FrostedLib"
		],
		"client": [
			"com.futurefrost.frostedlib.FrostedLibClient"
		],
		"server": [
			"com.futurefrost.frostedlib.FrostedLibServer"
		],
		"fabric-datagen": [
			"com.futurefrost.frostedlib.FrostedLibDataGenerator"
		],
		"cardinal-components": [
			"com.futurefrost.frostedlib.registry.ModComponents"
		]
	},
	"depends": {
		"fabricloader": ">=0.15.1",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*",
		"origins": ">=1.10.0"
	},
	"suggests": {
		"another-mod": "*"
	},
	"custom": {
		"cardinal-components": [
			"frostedlib:player_data",
			"frostedlib:entity_data"
		],
		"origins:power_types": [
			"frostedlib:save_pos",
			"frostedlib:return_pos",
			"frostedlib:fake_respawn",
			"frostedlib:dimension_shift"
		]
	}
}